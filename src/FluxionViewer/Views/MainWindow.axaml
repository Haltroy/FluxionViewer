<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:v="clr-namespace:FluxionViewer.Views;assembly=FluxionViewer"
        xmlns:l="clr-namespace:FluxionViewer.Lang"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="FluxionViewer.Views.MainWindow"
        TransparencyLevelHint="AcrylicBlur,Blur,Transparent,Mica,None"
        Icon="/Assets/logo.png"
        Closing="Window_OnClosing"
        Title="{x:Static l:Lang.AppName}">
    <v:MainView Name="Main" OnDesktop="True" />
</Window>